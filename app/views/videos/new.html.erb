<%= form_with model: @video do |f| %>
  <%= f.label :title, 'Título' %>
  <%= f.text_field :title %>
  <%= f.label :description, 'Descrição' %>
  <%= f.text_field :description %>
  <%= f.label :clip, 'Video' %>
  <%= f.file_field :clip %>
  <%= f.label :thumbnail, 'Thumbnail' %>
  <%= f.file_field :thumbnail %>
  <%= f.submit 'Upload' %>
<% end %>